---
import type { MarkdownHeading } from "astro";
import Headings from "@components/Headings.astro";

type Props = {
    headings: MarkdownHeading[];
};

const { headings } = Astro.props;
---

<div
    class="flex flex-col-reverse items-center gap-20 xl:flex-row xl:items-start xl:justify-center"
>
    <article class="prose-h2:heading prose prose-zinc dark:prose-invert">
        <!--render <Content /> here -->
        <slot />
    </article>
    <Headings headings={headings} />
</div>
